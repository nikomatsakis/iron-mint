# Iron Mint Git Configuration for ~/dev directory

[user]
    name = Niko Matsakis
    email = niko@alum.mit.edu

[core]
    editor = vim
    autocrlf = input
    excludesfile = $HOME/dev/iron-mint/config/gitignore-global

[init]
    defaultBranch = main

[pull]
    rebase = true

[merge]
    conflictstyle = diff3